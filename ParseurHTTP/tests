========== Utiles pour le générateur automatique ==========

tchar = 
    0123456789qwfpgjluyarstdhneiozxcvbkmQWFPGJLUYARSTDHNEIOZXCVBKM!#$%&'*+-.^_`|~

unreserved =
    0123456789qwfpgjluyarstdhneiozxcvbkmQWFPGJLUYARSTDHNEIOZXCVBKM-._~

HEXDIG =
    0123456789abcdefABCDEF

sub-delims = 
    !$&'()*+,;=

cookie-octet =
	!#$%&'()*+-./0123456789:<=>?@QWFPGJLUYARSTDHNEIOZXCVBKM[]^_`qwfpgjluyarstdhneiozxcvbkm{}|~

qdtext = HTAB + SP +
	!#$%&'()*+,-./0123456789:<;=>?@QWFPGJLUYARSTDHNEIOZXCVBKM[]^_`qwfpgjluyarstdhneiozxcvbkm{}|~

quoted-pair = HTAB + SP +
	every printable character